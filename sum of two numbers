import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        Scanner s = new Scanner (System.in);
        System.out.println("Enter the number1 :");
        int a = s.nextInt();
        System.out.println("Enter the number2 :");
        int b = s.nextInt();
        int c = a+b;
        System.out.println(c);
    }
}
